  "scripts": {
    "build": "cross-env NODE_ENV=production tsc && webpack --config ./webpack.config.prod.js",
    "build:webpack": "cross-env NODE_ENV=production webpack --config ./webpack.config.prod.js",
    "build:ts": "cross-env NODE_ENV=production tsc",
    "build:docs": "gulp build-docs && doctoc --notitle ./src/common/docs/docs.md",
    "build:client": "cross-env NODE_ENV=production webpack --config ./webpack.config.prod.js",
    "db:dump": "cross-env COMMAND=dump_db PGPASSWORD=pgadmin ts-node ./tools/db-manager.ts",
    "db:restore": "cross-env COMMAND=restore_db PGPASSWORD=pgadmin ts-node ./tools/db-manager.ts",
    "db:drop": "cross-env COMMAND=drop_dev_dbs PGPASSWORD=pgadmin ts-node ./tools/db-manager.ts",
    "db:generate": "cross-env COMMAND=generate_scripts PGPASSWORD=pgadmin ts-node ./tools/db-manager.ts",
    "db:install": "cross-env COMMAND=create_dev_dbs PGPASSWORD=pgadmin ts-node ./tools/db-manager.ts",
    "db:reinstall": "npm run db:drop && npm run db:install",
    "hot-server": "webpack-dev-server --hot --config ./webpack.config.dev.js",
    "nodemon:dump": "nodemon --dump",
    "start": "node ./dist/src/server/index.js",
    "start:dev": "cross-env NODE_ENV=development ts-node ./src/server/index.ts",
    "test": "cross-env NODE_ENV=test mocha ./test/unit/**/*.ts --exit",
    "test:all": "cross-env NODE_ENV=test mocha POSTGRES_DATABASE=aboutdevs_tests ./test/unit/*Spec.ts ./test/integration/*Spec.ts --exit",
    "test:integration": "cross-env NODE_ENV=test POSTGRES_DATABASE=aboutdevs_tests mocha ./test/integration/*Spec.ts --exit",
    "test:p": "cross-env NODE_ENV=test mocha --exit",
    "tools:massive-repl": "massive -c postgres://aboutdevs:aboutdevs@localhost:5432/indiejobs",
    "tools:print-config": "ts-node ./tools/print-config.ts",
    "tools:fake": "cross-env NODE_ENV=development ts-node ./tools/fake.ts",
    "tools:doctoc": "doctoc --notitle ./src/common/docs/docs.md",
    "tslint": "tslint --project ./tslint.json",
    "tslint:fix": "tslint --project ./tslint.json --fix",
    "gulp:build-docs": "gulp build-docs",
    "postinstall": "tsc && webpack --config ./webpack.config.prod.js"
  }
}
