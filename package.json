  "scripts": {
    "build-ts": "tsc",
    "db:clean": "cross-env COMMAND=clean_up_dev_dbs PGPASSWORD=pgadmin ts-node ./tools/db-manager.ts",
    "db:drop": "cross-env COMMAND=drop_dev_dbs PGPASSWORD=pgadmin ts-node ./tools/db-manager.ts",
    "db:generate": "cross-env COMMAND=generate_scripts PGPASSWORD=pgadmin ts-node ./tools/db-manager.ts",
    "db:install": "cross-env COMMAND=create_dev_dbs PGPASSWORD=pgadmin ts-node ./tools/db-manager.ts",
    "db:reinstall": "npm run db:drop && npm run db:install",
    "hot-server": "webpack-dev-server --hot --config ./webpack.config.dev.js",
    "nodemon:dump": "nodemon --dump",
    "start": "cross-env NODE_ENV=development ts-node ./src/server/index.ts",
    "start:watch": "cross-env NODE_ENV=development nodemon",
    "test": "cross-env NODE_ENV=test mocha ./test/unit/**/*.ts --exit",
    "test:all": "cross-env NODE_ENV=test mocha ./test/unit/*Spec.ts ./test/integration/*Spec.ts --exit",
    "test:integration": "cross-env NODE_ENV=test mocha ./test/integration/*Spec.ts --exit",
    "test:p": "cross-env NODE_ENV=test mocha --exit",
    "tools:apply-feminine-to-professions": "ts-node ./tools/apply-feminine-to-professionals.ts",
    "tools:massive-repl": "massive -c postgres://indiejobs:indiejobs@localhost:5432/indiejobs",
    "tools:print-config": "ts-node ./tools/print-config.ts",
    "tools:save-professions": "cross-env NODE_ENV=development ts-node ./tools/save-professions.ts",
    "tools:validate-professions": "cross-env NODE_ENV=development ts-node ./tools/validate-professions.ts",
    "tools:fake": "cross-env NODE_ENV=development ts-node ./tools/fake.ts",
    "tslint": "tslint ./src/**/*.ts ./test/**/*.ts",
    "tslint:fix": "tslint ./src/**/*.ts ./test/**/*.ts --fix"
  }
}
