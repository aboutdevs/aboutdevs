{
  "name": "indiejobs",
  "version": "0.0.1",
  "description": "Freelancer job search made ridiculously easy",
  "main": "index.js",
  "scripts": {
    "start": "cross-env NODE_ENV=development babel-node ./src/server/index.js",
    "start-watch": "cross-env NODE_ENV=development nodemon --exec \"babel-node ./src/server/index.js\"",
    "hot-server": "webpack-dev-server --hot --config ./webpack.config.dev.js",
    "test": "cross-env NODE_ENV=test mocha ./test/unit ",
    "test-integration": "cross-env NODE_ENV=test mocha ./test/integration/*Spec.js",
    "test-all": "cross-env NODE_ENV=test mocha ./test/unit ./test/integration/*Spec.js",
    "test-p": "cross-env NODE_ENV=test mocha",
    "lint-fix": "eslint --fix ./src ./test ./tools ./db",
    "db-install": "cross-env COMMAND=create_dev_dbs PGPASSWORD=indiejobs node ./db/installDb.js",
    "db-drop": "cross-env COMMAND=drop_dev_dbs PGPASSWORD=indiejobs node ./db/installDb.js",
    "db-clean": "cross-env COMMAND=clean_up_dev_dbs PGPASSWORD=indiejobs node ./db/installDb.js",
    "db-generate": "cross-env COMMAND=generate_scripts PGPASSWORD=indiejobs node ./db/installDb.js",
    "db-reinstall": "npm run db-drop && npm run db-install",
    "tools-save-professions": "cross-env NODE_ENV=development babel-node ./tools/save-professions",
    "tools-validate-professions": "cross-env NODE_ENV=development babel-node ./tools/validate-professions",
    "tools-print-config": "babel-node ./tools/print-config.js",
    "tools-massive-repl": "massive -c postgres://indiejobs:indiejobs@localhost:5432/indiejobs",
    "tools-apply-feminine-to-professions": "babel-node ./tools/apply-feminine-to-professionals.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/andrerpena/radiscope.git"
  },
  "author": "Andre Pena",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/andrerpena/radiscope/issues"
  },
  "homepage": "https://github.com/andrerpena/radiscope#readme",
  "dependencies": {
    "@types/passport": "^0.3.3",
    "awesome-typescript-loader": "^3.1.3",
    "axios": "^0.16.1",
    "babel-preset-es2016": "^6.24.1",
    "body-parser": "^1.17.2",
    "colors": "^1.1.2",
    "cookie-parser": "^1.4.3",
    "cookie-session": "^2.0.0-alpha.1",
    "express": "^4.15.3",
    "font-awesome": "^4.6.3",
    "immutable": "^3.8.1",
    "massive": "^3.0.0-rc1",
    "normalize.css": "^7.0.0",
    "passport": "^0.3.2",
    "passport-google-oauth": "^1.0.0",
    "prop-types": "^15.5.8",
