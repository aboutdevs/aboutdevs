{
  "name": "indiejobs",
  "version": "0.0.1",
  "description": "Freelancer job search made ridiculously easy",
  "main": "index.js",
  "scripts": {
    "start": "cross-env NODE_ENV=development babel-node ./src/server/index.js",
    "start-watch": "cross-env NODE_ENV=development nodemon --exec \"babel-node ./src/server/index.js\"",
    "hot-server": "webpack-dev-server --hot --config ./webpack.config.dev.js",
    "test": "mocha ./test --compilers js:babel-register",
    "test-db": "cross-env NODE_ENV=development mocha --timeout 15000 --compilers js:babel-register ./test/integration/db/*Spec.js",
    "lint-fix": "eslint --fix ./src ./test ./tools",
    "db-install": "cross-env COMMAND=create_dev_dbs PGPASSWORD=indiejobs node ./db/installDb.js",
    "db-drop": "cross-env COMMAND=drop_dev_dbs PGPASSWORD=indiejobs node ./db/installDb.js",
    "db-generate": "cross-env COMMAND=generate_scripts PGPASSWORD=indiejobs node ./db/installDb.js",
    "db-reinstall": "npm run db-drop && npm run db-install",
    "get-geo-data": "cross-env NODE_ENV=development babel-node ./tools/get-geo-data.js",
    "save-geo-data": "cross-env NODE_ENV=development babel-node ./tools/save-geo-data"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/andrerpena/radiscope.git"
  },
  "author": "Andre Pena",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/andrerpena/radiscope/issues"
  },
  "homepage": "https://github.com/andrerpena/radiscope#readme",
  "dependencies": {
    "axios": "^0.16.1",
    "bluebird": "^3.4.6",
    "body-parser": "^1.15.2",
    "colors": "^1.1.2",
    "cookie-parser": "^1.4.3",
    "cookie-session": "^2.0.0-alpha.1",
    "express": "^4.15.2",
    "font-awesome": "^4.6.3",
    "immutable": "^3.8.1",
    "massive": "^2.6.0",
    "normalize.css": "^6.0.0",
    "passport": "^0.3.2",
    "passport-google-oauth": "^1.0.0",
    "react": "^15.5.4",
    "react-activity": "^1.1.1",
    "react-dom": "^15.5.4",
    "react-modal": "^1.7.7",
    "react-redux": "^5.0.4",
    "react-router": "^3.0.2",
    "react-router-redux": "^4.0.8",
    "react-select": "^1.0.0-rc.3",
    "react-toggle-button": "^2.1.0",
    "redux": "^3.6.0",
    "redux-thunk": "^2.2.0"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-core": "^6.24.1",
    "babel-loader": "^6.3.2",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-polyfill": "^6.9.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-react-hmre": "^1.1.1",
    "babel-preset-stage-0": "^6.24.1",
    "body-parser": "^1.17.1",
    "chai": "^3.5.0",
    "child-process-promise": "^2.2.1",
    "classnames": "^2.2.5",
    "cross-env": "^3.2.3",
    "css-loader": "^0.28.0",
    "eslint": "^3.19.0",
    "eslint-config-airbnb": "^14.1.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^4.0.0",
    "eslint-plugin-react": "^6.10.3",
    "extract-text-webpack-plugin": "^2.0.0",
    "file-loader": "^0.11.1",
    "fs-extra-promise": "^0.4.0",
    "json-loader": "^0.5.4",
    "mocha": "^3.0.2",
    "node-sass": "^4.5.2",
    "nodemon": "^1.11.0",
    "npm": "^3.3.4",
    "path": "^0.12.7",
    "raw-loader": "^0.5.1",
    "react-hot-loader": "^3.0.0-beta.3",
    "rimraf-promise": "^2.0.0",
    "sass-loader": "^6.0.3",
    "style-loader": "^0.16.1",
    "underscore": "^1.8.3",
    "webpack": "^2.4.1",
    "webpack-dev-server": "^2.4.3"
  }
}
