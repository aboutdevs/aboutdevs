{
    "extends": [
        "tslint:latest",
        "tslint-react"
    ],
    "rules": {
        "quotemark": [
            true,
            "'"
        ],
        "max-line-length": [
            false
        ],
        "curly": false,
        "arrow-parens": [
            false
        ],
        "object-literal-sort-keys": false,
        "member-ordering": [
            false
        ],
        "jsx-no-lambda": [
            false
        ]
    }
}